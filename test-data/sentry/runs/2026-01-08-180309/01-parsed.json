        "file": "docker-compose.yml",
        "format": "docker-compose"
      },
      "image": "registry.gitlab.com/egos-tech/smtp",
      "volumes": [
        {
          "source": "sentry-smtp",
          "target": "/var/spool/exim4",
          "type": "volume"
